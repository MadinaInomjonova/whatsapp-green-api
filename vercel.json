{
  "version": 2,
  "static": "./node_modules/json-server/public/index.html",
  "rewrites": [{ "source": "/(.*)", "destination": "./public/index.html" }]
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["db.json"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ]
}
